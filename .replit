
modules = ["python-3.12", "bash"]

[nix]
channel = "stable-24_05"

[deployment]
run = "python webui.py --ip 0.0.0.0 --port 5000"
deploymentTarget = "cloudrun"

[env]
PYTHONPATH = "."
#
[languages]
python = "3.12"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 6080
externalPort = 6080

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 3706638
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "/nix/store/python39/bin/python3.9 webui.py --ip 0.0.0.0 --port 5000"
